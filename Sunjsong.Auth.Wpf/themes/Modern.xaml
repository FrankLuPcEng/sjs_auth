<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="SystemAccentColor">#FF2563EB</Color>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource SystemAccentColor}" />
    <SolidColorBrush x:Key="AccentBrushSecondary" Color="#FF1E40AF" />
    <SolidColorBrush x:Key="SurfaceBrush" Color="#0F000000" />
</ResourceDictionary>
